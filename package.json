{
  "name": "zjffun",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "main": "n/a",
  "scripts": {
    "clean-dist": "rimraf ./dist",
    "clean": "npm run clean-dist && rimraf src/content/**/_*.md src/**/_*.json",
    "start": "npm run clean-dist && cross-env NODE_ENV=development webpack-dev-server --config webpack.dev.js --env.dev",
    "update-repos": "node src/utilities/fetch-package-repos.js",
    "content": "node src/scripts/build-content-tree.js ./src/blogs ./src/blogs/_blogs.json",
    "build-test": "npm run build && http-server dist/",
    "fetch": "run-p fetch:*",
    "fetch:starter-kits": "node src/utilities/fetch-starter-kits.js",
    "prebuild": "npm run clean",
    "build": "run-s fetch content && cross-env NODE_ENV=production webpack --config webpack.prod.js",
    "postbuild": "npm run sitemap",
    "test": "npm run lint",
    "lint": "run-s lint:*",
    "lint:js": "eslint src --ext .js,.jsx --cache true --cache-location .cache/.eslintcache",
    "lint:md": "remark . --quiet --frail ",
    "fix:md": "remark . -o --quiet --frail",
    "sitemap": "cd dist && sitemap-static --prefix=https://www.zjffun.com/ > sitemap.xml",
    "serve": "npm run build && sirv start ./dist --port 4000",
    "deploy": "gh-pages -d dist -o coding -b master",
    "bp": "npm run build && npm run deploy"
  },
  "author": "zjf",
  "license": "CC BY 4.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/zjffun/zjffun.git"
  },
  "bugs": {
    "url": "https://github.com/zjffun/zjffun/issues"
  },
  "engines": {
    "node": ">=8.9.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "npm run lint:js"
    ],
    "*.md": [
      "npm run lint:md"
    ]
  },
  "devDependencies": {
    "@mdx-js/loader": "0.15.7",
    "@mdx-js/mdx": "0.15.7",
